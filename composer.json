  "require": {
    "php": ">=5.6.0",
    "illuminate/container": "^4.0",
    "illuminate/support": "^4.0",
    "illuminate/config": "^4.0",
    "superbalist/php-pubsub": "^1.0",
    "superbalist/php-pubsub-redis": "^1.0",
    "superbalist/php-pubsub-google-cloud": "^2.0|^3.0"
